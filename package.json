	"license": "MIT",
	"engines": {
		"node": ">= 20.11.1",
		"npm": ">= 10.2.4"
	},
	"scripts": {
		"start:demo-app": "nx serve demo-app",
		"build:demo-app": "nx run demo-app:build",
		"show:demo-app": "nx show project demo-app --web",
		"check": "biome check apps libs",
		"check:apply": "biome check --apply apps libs",
		"format": "nx run-many --target=format --all --parallel",
		"lint": "nx run-many --target=lint --all --parallel",
		"lint:apps": "nx run-many --target=lint --projects=apps/* --parallel",
		"lint:libs": "nx run-many --target=lint --projects=libs/* --parallel",
		"release:demo-app:patch": "nx run demo-app:version --releaseAs=patch",
		"release:demo-app:minor": "nx run demo-app:version --releaseAs=minor",
		"release:demo-app:major": "nx run demo-app:version --releaseAs=major",
		"release:demo-app:dry-run": "nx run demo-app:version --dry-run",
		"release:push": "git push --follow-tags origin",
		"prepare": "husky install"
	},
	"private": true,
